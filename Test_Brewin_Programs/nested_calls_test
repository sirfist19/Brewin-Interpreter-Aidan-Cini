(class main
    (method main ()
        (begin
          (print (call (new other) func2 5))
          (print (call (call (new other) func1 3) func2 5))
        )
    ) 
) 
(class other
    (method func1 (x) 
        (return (new other))
    )
    (method func2 (x)
        (begin
            (print x)
            (return 0)
        )
    )
)