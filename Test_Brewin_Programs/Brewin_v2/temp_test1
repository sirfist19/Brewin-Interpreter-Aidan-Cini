(class b
  (method void main () (return))
)
(class d inherits b
  (method void main () (return))
)
(class dd inherits d
  (method void main () (return))
)
(class ddd inherits dd
  (method void main () (return))
)

(class main
  (field ddd a null)
 (method d foo () 
   (return null))
 (method void main ()
  (set a (call me foo))
 )
)