(class main
    (method main ()
        (begin
            (print "5 + 4 = 9: " (+ 5 4))
            (print "5 * 4 = 20: " (* 5 4))
            (print "5 - 4 = 1: " (- 5 4))
            (print "-4 + 5 = -1: " (- 4 5))
            (print "5 / 4 = 1: " (/ 5 4))
            (print "-5 / 4 = -2: " (/ -5 4))
            (print "! true = false:" (! true))
            (print "! false = true:" (! false))
            (print "false:" (& true false))
            (print "true:" (& true true))
            (print "true:" (| true false))
            (print "true:" (| true true))
            (print "false:" (| false false))
            (print "! true = false:" (! true))
            (print "! false = true:" (! false))
            (print "0 > 5 = false:" (> 0 5))
            (print "0 >= 5 = false:" (>= 0 5))
            (print "0 < 5 = true:" (< 0 5))
            (print "0 <= 5 = true:" (<= 0 5))
            (print "foobar:" (+ "foo" "bar"))

            (print "\nNested call tests!")
            (print "8:" (+ 5 (+ 1 2)))
            (print "(| (> 0 -5) (<= 0 -5)) -> true: " (| (> 0 -5) (<= 0 -5)))
            (print "true:" (& 
                (& true (| (> 0 -5) (<= 0 -5)))
                (| false true)
                ))
        )
    ) 
) 